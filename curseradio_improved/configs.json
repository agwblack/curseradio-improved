{
  "opml": { "root": "http://opml.radiotime.com/" },
  "playback": { "command": "/usr/bin/mpv" },
  "keymap": {
    "up": ["KEY_UP", "k"],
    "down": ["KEY_DOWN", "j"],
    "start": ["KEY_HOME", "g"],
    "end": ["KEY_END", "G"],
    "pageup": ["KEY_PPAGE", "p"],
    "pagedown": ["KEY_NPAGE", "n"],
    "enter": ["KEY_ENTER", "l", "KEY_RIGHT"],
    "stop": "s",
    "exit": "q",
    "favourite": "f"
  },
  "colors": {
    "selected-item": "inverted",
    "message": "yellow",
    "statusbar": "green-bg"
  },
  "opened_delimiter": "📁",
  "closed_delimiter": "📂",
  "statusbar-text": " ♫ {} ♫ ",
  "outline-color": "green",
  "title-color": "red"
}
